<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox alignment="CENTER_LEFT" prefHeight="30.0" prefWidth="440.0" style="-fx-background-color: FFEEB6; -fx-spacing: 10;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quizzey.quizzey.StudentAvailableQuizzesCardController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <Text fx:id="txtQuizName" strokeType="OUTSIDE" strokeWidth="0.0" style="-fx-fill: #916CF5;" text="Quiz Name">
      <font>
         <Font size="18.0" />
      </font>
      <HBox.margin>
         <Insets left="10.0" />
      </HBox.margin>
   </Text>
   <TextField fx:id="txtFieldCode" prefHeight="24.0" prefWidth="86.0">
      <HBox.margin>
         <Insets left="90.0" />
      </HBox.margin>
   </TextField>
   <Button fx:id="btnEnter" mnemonicParsing="false" prefHeight="5.0" prefWidth="99.0" text="Enter">
      <HBox.margin>
         <Insets left="20.0" />
      </HBox.margin>
   </Button>
</HBox>
